{
  "name": "abp-zero-template",
  "version": "10.5.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "docker-build": "docker build -t fbp-client .",
    "docker-run": "docker run -it --rm -d -p 8080:80 --name fbp-client fbp-client",
    "ng": "ng",
    "preinstall": "node -e \"if(process.env.npm_execpath.indexOf('yarn') === -1) throw new Error('You must use Yarn to install, not NPM')\"",
    "publish": "gulp build && ng build --prod",
    "publish:dev": "gulp build && ng build --configuration=dev",
    "publish:qa": "node --version && gulp build && ng build --configuration=qa",
    "publish:qa:debug": "node --version && gulp build && ng build --configuration=qa && tree dist/",
    "start": "ng serve --host 0.0.0.0 --port 4200",
    "hmr": "gulp buildDev && ng serve --host 0.0.0.0 --port 4200 --hmr",
    "test": "gulp buildDev && ng test --source-map",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "gulp buildDev && protractor",
    "create-dynamic-bundles": "gulp buildDev",
    "build:stats": "ng build --stats-json",
    "preanalyze": "npm run build:stats",
    "analyze": "webpack-bundle-analyzer dist/stats.json",
    "nswag": "cd nswag/ && refresh.bat",
    "nswag-sh": "cd nswag/ && ./refresh.sh",
    "generate-api-client:content": "export JAVA_OPTS=\"-Dio.swagger.parser.util.RemoteUrl.trustAll=true -Dio.swagger.v3.parser.util.RemoteUrl.trustAll=true\" && npx @openapitools/openapi-generator-cli generate -g typescript-angular -i https://ipaas-content-dev-useast-api.azurewebsites.net/swagger/v1/swagger.json -o src/app/shared/common/apis/generated/content",
    "generate-api-client:content-localfile": "npx @openapitools/openapi-generator-cli generate -i openapi-content.json -g typescript-angular -o src/app/shared/common/apis/generated/content",
    "generate-api-client:features": "export JAVA_OPTS=\"-Dio.swagger.parser.util.RemoteUrl.trustAll=true -Dio.swagger.v3.parser.util.RemoteUrl.trustAll=true\" && npx @openapitools/openapi-generator-cli generate -g typescript-angular -i https://ipaas-schoolfeatures-dev-useast-api.azurewebsites.net/swagger/v1/swagger.json -o src/app/shared/common/apis/generated/features",
    "generate-api-client:features-localfile": "npx @openapitools/openapi-generator-cli generate -i openapi_schoolfeatures.json -g typescript-angular -o src/app/shared/common/apis/generated/features",
    "generate-api-client:faculty": "export JAVA_OPTS=\"-Dio.swagger.parser.util.RemoteUrl.trustAll=true -Dio.swagger.v3.parser.util.RemoteUrl.trustAll=true\" && npx @openapitools/openapi-generator-cli generate -g typescript-angular -i https://ipaas-faculty-dev-useast-api.azurewebsites.net/swagger/v1/swagger.json -o src/app/shared/common/apis/generated/faculty",
    "generate-api-client:faculty-localfile": "npx @openapitools/openapi-generator-cli generate -i openapi_faculty.json -g typescript-angular -o src/app/shared/common/apis/generated/faculty",
    "generate-api-client:testimonials": "export JAVA_OPTS=\"-Dio.swagger.parser.util.RemoteUrl.trustAll=true -Dio.swagger.v3.parser.util.RemoteUrl.trustAll=true\" && npx @openapitools/openapi-generator-cli generate -g typescript-angular -i https://ipaas-testimonials-dev-useast-api.azurewebsites.net/swagger/v1/swagger.json -o src/app/shared/common/apis/generated/testimonials",
    "generate-api-client:testimonials-localfile": "npx @openapitools/openapi-generator-cli generate -i openapi-testimonials.json -g typescript-angular -o src/app/shared/common/apis/generated/testimonials",
    "generate-api-client:careers-localfile": "npx @openapitools/openapi-generator-cli generate -i openapi-careers.json -g typescript-angular -o src/app/shared/common/apis/generated/careers",
    "generate-api-client:school-events-localfile": "npx @openapitools/openapi-generator-cli generate -i openapi-school-events.json -g typescript-angular -o src/app/shared/common/apis/generated/school-events",
    "start-secure": "npx cross-env ng serve --host 0.0.0.0 --port 4200 --hmr --ssl true --ssl-cert \"$LOCAL_CERT\" --ssl-key \"$LOCAL_KEY\""
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "^15.2.9",
