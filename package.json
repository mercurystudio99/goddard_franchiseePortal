    "ngx-bootstrap": "^8.0.0",
    "ngx-captcha": "^11.0.0",
    "ngx-image-cropper": "^3.3.5",
    "ngx-perfect-scrollbar": "^10.1.1",
    "ngx-spinner": "^12.0.0",
    "object-path": "^0.11.4",
    "primeicons": "^4.1.0",
    "primeng": "15",
    "push.js": "^1.0.12",
    "quill": "^1.3.7",
    "rtl-detect": "^1.0.3",
    "rxjs": "^6.6.7",
    "simple-line-icons": "^2.4.1",
    "sweetalert2": "^11.0.18",
    "text-mask-addons": "^3.8.0",
    "tslib": "^2.3.0",
    "webdriver-manager": "^12.1.8",
